{
    "compilerOptions": {
      "experimentalDecorators": true,
      "target": "es5",
      "module": "commonjs",
      "outDir": "dist",
      "strict": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strictPropertyInitialization": false,
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }